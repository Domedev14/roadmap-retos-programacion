public class devdomene14 {

    public static void main(String[] args) {
        //
        // Esto es un comentrio de una linea

        /*
         * Esto es un
         * comentario
         * de varias lineas
         */

        // Declaración de variable
        String my_first_variable = "Mi primera variable";
        // Declaración de constante
        final float gravedad = 9.8f;

        // tipos de datos primitivos
        // byte
        byte b = 0;
        // short
        short s = 0;
        // Entero
        int i = 0;
        // long
        long l = 0;
        // float
        float f = 0.0f;
        // double
        double d = 0.0d;
        // caracter
        char c = 'c';
        // cadena
        String cadena = "Java";
        // booleano
        boolean bool = true;

        System.out.println("Hola, Java");
    }

}
